{
  "name": "maru-apply-ai",
  "displayName": "Maru apply ai",
  "version": "0.0.1",
  "description": "A simple Chrome extension built with React",
  "author": "Josip N",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "@types/chrome": "^0.0.268",
    "pdfjs-dist": "^5.4.394",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "identity",
      "storage",
      "sidePanel"
    ],
    "action": {},
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "assets/pdf.worker.min.mjs"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
